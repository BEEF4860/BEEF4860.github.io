  <script>
   function handleButtonClick(buttonType) {
        if (buttonType === 'discord') {
          window.location.href = "https://discord.gg/UD5uEtsQsx";
        } else if (buttonType === 'youtube') {
          window.location.href = "https://www.youtube.com/@DiplomacyMinecraft";
        }
      }
      
      function displayWelcomeMessage() {
        alert("Welcome to DiplomacyMC");
      }
      
       document.getElementById('discordButton').addEventListener('click', function() {
        handleButtonClick('discord');
      });

      document.getElementById('youtubeButton').addEventListener('click', function() {
        handleButtonClick('youtube');
      });

  
